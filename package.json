{
  "name": "@freakzlike/i18n-extract",
  "version": "0.0.1",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/freakzlike/i18n-extract.git"
  },
  "type": "module",
  "files": [
    "dist"
  ],
  "main": "./dist/i18n-extract.cjs",
  "bin": {
    "i18n-extract": "./bin/cli.js"
  },
  "module": "./dist/i18n-extract.js",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/i18n-extract.js",
      "require": "./dist/i18n-extract.cjs"
    }
  },
  "scripts": {
    "build": "rollup -c rollup.config.ts --bundleConfigAsCjs",
    "test": "vitest",
    "lint": "eslint src/ tests/ --ext .ts --ignore-path .gitignore",
    "lint:fix": "pnpm run lint --fix",
    "demo:cli": "node bin/cli.cjs tests/example/i18n-extract.config.cjs"
  },
  "dependencies": {
    "glob": "^10.2.6"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "25.0.0",
    "@rollup/plugin-node-resolve": "15.1.0",
    "@rollup/plugin-typescript": "11.1.1",
    "@tsconfig/node18": "2.0.1",
    "@types/node": "18.16.16",
    "@typescript-eslint/eslint-plugin": "5.59.8",
    "@typescript-eslint/parser": "5.59.8",
    "eslint": "8.42.0",
    "rollup": "3.23.0",
    "tslib": "2.5.3",
    "typescript": "5.1.3",
    "vite": "4.3.9",
    "vitest": "0.31.4"
  }
}
